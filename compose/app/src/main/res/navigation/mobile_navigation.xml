<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    >

    <activity
        android:id="@+id/photoActivity"
        android:name="com.longforus.cpix.PhotoActivity"
        android:label="PhotoActivity" />
    <activity
        android:id="@+id/videoActivity"
        android:name="com.longforus.cpix.VideoPlayerActivity"
        android:label="VideoPlayerActivity" />

    <activity
        android:id="@+id/mainActivity"
        android:name="com.longforus.cpix.MainActivity"
        android:label="MainActivity" />

    <composable
        android:id="@+id/toPhotoCom"
        android:label="PhotoScreen"
        app:route="photo" />

    <action
        android:id="@+id/go2Photo"
        app:destination="@id/toPhotoCom" />
</navigation>